        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DD.ClientWPF"
        mc:Ignorable="d"
        Title="Алерты" Height="600" Width="440" ResizeMode="CanMinimize" Background="#FFFFB7B7">

    <Grid>
        <ScrollViewer>
